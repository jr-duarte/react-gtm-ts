{
  "name": "react-gtm-ts",
  "author": "Junior Duarte",
  "license": "MIT",
  "version": "1.0.44",
  "description": "This is a library to facilitate the implementation of Google Tag Manager.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jr-duarte/react-gtm-ts"
  },
  "keywords": [
    "Google Tag Manager",
    "GTM",
    "Google Analytics",
    "GA",
    "TAG",
    "tag",
    "react",
    "next",
    "ts",
    "typescript"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --minify",
    "dev": "tsup src/index.ts --format esm,cjs --dts --minify --watch",
    "prepare": "husky install",
    "lint": "eslint ./",
    "lintFix": "eslint ./ --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "prettier": "^2.7.1",
    "tsup": "^6.5.0",
    "typescript": "*"
  }
}
